{
  "name": "quiq-chat",
  "version": "2.14.3",
  "descri1tion": "Library to help with network requests to create a webchat client for Quiq Messaging",
  "main": "dist/quiq-chat.js",
  "module": "lib/quiq-chat.js",
  "repository": "https://github.com/Quiq/quiq-chat",
  "scripts": {
    "start": "tsc --watch",
    "lint": "tslint --config tslint.json --project tsconfig.json 'src/**/*.ts'",
    "test": "jest --env=jsdom --coverage && yarn run lint",
    "jest": "jest --watch --env=jsdom --runInBand",
    "build": "rimraf ./lib ./dist && tsc && rollup -c",
    "prepublish": "yarn test && yarn build",
    "precommit": "lint-staged",
    "prettier": "prettier --write src/**/*.ts src/*.ts"
  },
  "lint-staged": {
    "src/**/*.js": [
      "prettier --write",
      "git add"
    ]
  },
  "jest": {
    "modulePaths": [
      "src",
      "node_modules"
    ],
    "rootDir": ".",
    "setupFiles": [
      "./jest.setup.js",
      "jest-localstorage-mock"
    ],
    "notify": true,
    "roots": [
      "<rootDir>/src"
    ],
    "transform": {
      "^.+\\.(t|j)sx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.tsx?$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "tsConfigFile": "tsconfig.jest.json",
        "useBabelrc": true
      }
    }
  },
  "keywords": [
    "quiq"
  ],
  "author": "nate.norberg@goquiq.com",
  "license": "MIT",
  "dependencies": {
    "isomorphic-fetch": "2.2.1",
    "jstimezonedetect": "1.0.6",
    "jwt-decode": "2.2.0",
    "lodash": "4.17.10",
    "loglevel": "1.4.1",
    "loglevel-plugin-prefix": "0.5.3",
    "quiq-socket": "0.0.4",
    "store": "2.0.12",
    "stubborn-fetch": "0.1.0",
    "ua-parser-js": "0.7.18",
    "url-parse": "1.4.3"
  },
  "devDependencies": {
    "@babel/plugin-proposal-class-properties": "7.3.0",
    "@babel/plugin-proposal-decorators": "7.3.0",
    "@babel/plugin-proposal-do-expressions": "7.2.0",
    "@babel/plugin-proposal-export-default-from": "7.2.0",
    "@babel/plugin-proposal-export-namespace-from": "7.2.0",
    "@babel/plugin-proposal-function-bind": "7.2.0",
    "@babel/plugin-proposal-function-sent": "7.2.0",
    "@babel/plugin-proposal-json-strings": "7.2.0",
    "@babel/plugin-proposal-logical-assignment-operators": "7.2.0",
    "@babel/plugin-proposal-nullish-coalescing-operator": "7.2.0",
    "@babel/plugin-proposal-numeric-separator": "7.2.0",
    "@babel/plugin-proposal-optional-chaining": "7.2.0",
    "@babel/plugin-proposal-pipeline-operator": "7.3.2",
    "@babel/plugin-proposal-throw-expressions": "7.2.0",
    "@babel/plugin-syntax-dynamic-import": "7.2.0",
    "@babel/plugin-syntax-import-meta": "7.2.0",
    "@babel/preset-env": "7.3.1",
    "@babel/preset-flow": "7.0.0",
    "@types/isomorphic-fetch": "0.0.34",
    "@types/jest": "23.3.1",
    "@types/jstimezonedetect": "1.0.1",
    "@types/jwt-decode": "2.2.1",
    "@types/lodash": "4.14.116",
    "@types/loglevel": "1.5.3",
    "@types/qs": "6.5.1",
    "@types/storejs": "2.0.3",
    "@types/ua-parser-js": "0.7.32",
    "@types/url-parse": "^1.4.1",
    "jest-localstorage-mock": "2.3.0",
    "quiq-scripts": "0.9.4",
    "rollup": "0.64.1",
    "rollup-plugin-babel": "^4.0.1",
    "rollup-plugin-includepaths": "0.2.3",
    "rollup-plugin-json": "^2.3.0",
    "ts-jest": "23.1.3",
    "tslint": "5.11.0",
    "tslint-config-airbnb-base": "0.2.0",
    "tslint-config-prettier": "1.14.0",
    "typescript": "2.9.2"
  }
}
