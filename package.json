{
  "name": "quiq-chat",
  "version": "1.22.0",
  "description":
    "Library to help with network requests to create a webchat client for Quiq Messaging",
  "main": "build/quiq-chat.js",
  "repository": "https://github.com/Quiq/quiq-chat",
  "scripts": {
    "start": "./node_modules/.bin/nodemon --watch src -e js,sass --exec npm run build",
    "lint": "node ./node_modules/eslint/bin/eslint.js src/",
    "test":
      "flow && jest --env=jsdom --coverage && node ./node_modules/eslint/bin/eslint.js src/ --quiet",
    "flow": "flow",
    "jest": "jest --watch --env=jsdom",
    "build": "rollup -c",
    "prepublish": "rollup -c",
    "precommit": "lint-staged"
  },
  "lint-staged": {
    "src/**/*.js": ["prettier --write", "git add"]
  },
  "jest": {
    "modulePaths": ["src", "node_modules"],
    "rootDir": ".",
    "setupFiles": ["./jest.setup.js"],
    "notify": true
  },
  "keywords": ["quiq"],
  "author": "nate.norberg@goquiq.com",
  "license": "MIT",
  "dependencies": {
    "atmosphere.js":
      "https://github.com/Quiq/atmosphere-js/tarball/cf35c913143cf2c391b6bcb694ae8a47794a1b88",
    "isomorphic-fetch": "2.2.1",
    "jwt-decode": "2.2.0",
    "lodash": "4.17.4",
    "loglevel": "1.4.1",
    "loglevel-plugin-prefix": "0.5.3",
    "qs": "6.5.0",
    "raven-js": "3.17.0",
    "store": "2.0.12",
    "ua-parser-js": "0.7.12"
  },
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-eslint": "^7.2.3",
    "babel-jest": "21.0.2",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-plugin-transform-es2015-modules-commonjs": "^6.24.1",
    "babel-plugin-transform-flow-strip-types": "^6.22.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "eslint": "3.19.0",
    "eslint-config-airbnb": "14.1.0",
    "eslint-plugin-flowtype": "2.11.4",
    "eslint-plugin-import": "2.2.0",
    "eslint-plugin-jsx-a11y": "4.0.0",
    "eslint-plugin-react": "6.9.0",
    "flow-bin": "0.54.1",
    "husky": "^0.13.4",
    "jest": "21.0.2",
    "jsdom": "^11.0.0",
    "lint-staged": "^3.6.0",
    "nodemon": "^1.11.0",
    "prettier": "1.6.1",
    "rollup": "^0.41.6",
    "rollup-plugin-babel": "^2.7.1",
    "rollup-plugin-flow": "^1.1.1",
    "rollup-plugin-includepaths": "^0.2.2",
    "rollup-plugin-json": "^2.3.0"
  }
}
